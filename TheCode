#include <iostream>
using namespace std;
int main()
{
   /* Write a program that asks the user to enter their age.
	If the age is 18 or older, print "Adult".
	Otherwise, print "Minor".
    */
    
      int age;
    cout << "Enter your age:";
    cin >> age;
    
    
    if(age >=18){
        cout << "Adult" << endl;
    }else{
        cout << "Minor" << endl;
    }
    return 0;
}
